## Build stage ##
FROM maven:3.5.3-jdk-alpine as build
WoRDIR /app
COPY . .
RUN mnv install -DskipTests=true

## run stage##
s
FROM amazon.....
WORKDIR /run
COPY --from=build /app/target/"sdsd".jar /run/"ds".jar

EXPOSE 8080

ENTRYPOINT java -jar /run/aada.jar
